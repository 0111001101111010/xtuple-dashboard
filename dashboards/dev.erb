<% content_for :title do %>xTuple Development Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="logo" data-view="Image" data-image="/xtuple-app.png" data-title="Developer Dashboard"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Countdown" data-title="Captain America" data-end="April 4, 2014 12:00:00"></div>
